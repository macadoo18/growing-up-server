{
    "name": "growing-up-server",
    "version": "1.0.0",
    "main": "app.js",
    "resolve": "1.8.1",
    "scripts": {
        "test": "mocha -R min -require test/setup.js",
        "dev": "nodemon src/server.js",
        "migrate": "postgrator --config postgrator-config.js",
        "migrate:test": "env NODE_ENV=test npm run migrate",
        "migrate:production": "env SSL=true NODE_TLS_REJECT_UNAUTHORIZED=0 DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
        "predeploy": "npm audit && npm run migrate:production",
        "deploy": "git push heroku master",
        "start": "node src/server.js"
    },
    "keywords": [],
    "engines": {
        "node": "12.13.0"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.0",
        "bcryptjs": "^2.4.3",
        "cloudinary": "^1.22.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-fileupload": "^1.1.10",
        "helmet": "^3.23.3",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.21.2",
        "moment": "^2.27.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "pg": "^8.3.0",
        "postgrator-cli": "^3.3.0",
        "serialize-javascript": "^3.1.0",
        "xss": "^1.0.7"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "mocha": "^8.1.1",
        "nodemon": "^2.0.4",
        "supertest": "^4.0.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/halemd30/growing-up-server.git"
    },
    "bugs": {
        "url": "https://github.com/halemd30/growing-up-server/issues"
    },
    "homepage": "https://github.com/halemd30/growing-up-server#readme",
    "description": ""
}
